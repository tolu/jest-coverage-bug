{
  "name": "jest-coverage-bug",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:tolu/jest-coverage-bug.git",
  "author": "Tobias Lundin <tobias.lundin@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-jest": "^20.0.1",
    "babel-preset-es2015": "^6.24.1",
    "jest": "^20.0.1"
  },
  "scripts": {
    "test": "jest --coverage --forceExit"
  },
  "jest": {
    "coverageDirectory": "./coverage",
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "testRegex": "spec\\.jsx?$",
    "testEnvironment": "node"
  }
}
